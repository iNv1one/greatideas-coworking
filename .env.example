# Environment variables for GreatIdeas project
DEBUG=True
SECRET_KEY=your-secret-key-here

# Database
DB_NAME=greatideas_db
DB_USER=postgres
DB_PASSWORD=your_password_here
DB_HOST=localhost
DB_PORT=5432

# Telegram Bot
TELEGRAM_BOT_TOKEN=your_bot_token_here
TELEGRAM_WEBHOOK_URL=https://yourdomain.com/cafe/telegram/webhook/

# Static and Media
STATIC_URL=/cafe/static/
MEDIA_URL=/cafe/media/
STATIC_ROOT=/var/www/cafe/static/
MEDIA_ROOT=/var/www/cafe/media/

# Redis (for Celery)
REDIS_URL=redis://localhost:6379/0